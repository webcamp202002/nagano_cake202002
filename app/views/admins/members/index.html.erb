<p>会員一覧</p>
<table>
	<tr>
		<th>会員ID</th>
		<th>氏名</th>
		<th>メールアドレス</th>
		<th>退会ステータス</th>
	</tr>
	<%if @members.present? %>
	<% @members.each do |member| %>
	<tr>
		<td><%= member.id %></td>
		<td>
		<%= link_to admins_member_path(member) do %>
		<%= member.last_name %><%= member.first_name %>
			<% end %>
		</td>
		<td><%= member.email %></td>
		<td><%= member.is_withdrow_i18n %></td>
	</tr>
	<% end %>
	<% end %>

</table>